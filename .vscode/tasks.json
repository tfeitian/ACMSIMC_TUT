{
  "version": "2.0.0",
  "tasks": [{
      "label": "build exe",
      "command": "make",
      "options": {
        "cwd": "${workspaceRoot}"
      },
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "problemMatcher": ["$gcc"]
    },
    {
      "label": "run",
      "command": "${workspaceRoot}\\build\\main.exe",
      "options": {
        "cwd": "${workspaceRoot}"
      }
    },
    {
      "label": "runscript",
      "command": "C:\\Users\\610099764\\AppData\\Local\\Programs\\Python\\Python36-32\\python.exe",
      "args": [
        "./scripts/ACMPlot.py"
      ],
      "options": {
        "cwd": "${workspaceRoot}"
      }
    },
    {
      "label": "Build and Run",
      "dependsOn": ["buildexe", "run", "runscript"],
      "dependsOrder": "sequence",
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "problemMatcher": ["$gcc"]
    }
  ]
}