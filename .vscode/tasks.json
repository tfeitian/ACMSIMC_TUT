{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "build exe",
      "command": "make",
      "options": {
        "cwd": "${workspaceRoot}"
      },
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "problemMatcher": [
        "$gcc"
      ]
    },
    {
      "label": "run",
      "command": "${workspaceRoot}\\build\\main.exe",
      "options": {
        "cwd": "${workspaceRoot}\\build"
      }
    },
    {
      "label": "runscript",
      "command": "C:\\Users\\610099764\\AppData\\Local\\Programs\\Python\\Python36-32\\python.exe",
      "args": [
        "./scripts/ACMPlot.py"
      ],
      "options": {
        "cwd": "${workspaceRoot}"
      }
    },
    {
      "label": "start server",
      "command": "C:\\Program Files\\nodejs\\node.exe",
      "args": [
        "server.js"
      ],
      "options": {
        "cwd": "${workspaceRoot}\\scripts\\node"
      },
      "problemMatcher": []
    },
    {
      "label": "Build and Run",
      "dependsOn": [
        "build exe",
        "run"
      ],
      "dependsOrder": "sequence",
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "problemMatcher": [
        "$gcc"
      ]
    }
  ]
}